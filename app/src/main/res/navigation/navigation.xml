    <action
        android:id="@+id/action_profileFragment_to_myProfileFragment"
        app:destination="@id/accountFragment" />
    <action
        android:id="@+id/action_profileFragment_to_historyFragment"
        app:destination="@id/historyFragment" />
    <action
        android:id="@+id/action_profileFragment_to_about"
        app:destination="@id/contactusActivity" />
    <action
        android:id="@+id/action_profileFragment_to_language"
        app:destination="@id/languageActivity" />
    <action
        android:id="@+id/action_detailsActivity_to_cartFragment"
        app:destination="@id/cartFragment" />
    <action
        android:id="@+id/action_cartFragment_to_homefragment"
        app:destination="@id/homeFragment" />
    <action
        android:id="@+id/action_homeFragment_to_FreshFishFragment"
        app:destination="@id/freshFishFragment" />
    <action
        android:id="@+id/action_homeFragment_to_DryFishFragment"
        app:destination="@id/dryFishFragment" />
    <action
        android:id="@+id/action_homeFragment_to_PicklesFragment"
        app:destination="@id/picklesFragment" />
</navigation>