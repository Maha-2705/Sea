<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">


    <fragment
        android:id="@+id/cartFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.FavouriteFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <activity
        android:id="@+id/contactusActivity"
        android:name="com.capztone.seafishfy.ui.activities.ContactusActivity"
        android:label="activity_contact"
        tools:layout="@layout/activity_contactus" />
    <activity
        android:id="@+id/languageActivity"
        android:name="com.capztone.seafishfy.ui.activities.LanguageActivity"
        android:label="activity_language"
        tools:layout="@layout/activity_language" />
    <activity
        android:id="@+id/detailsActivity"
        android:name="com.capztone.seafishfy.ui.activities.DetailsActivity"
        android:label="activity_contact"
        tools:layout="@layout/activity_details" />
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" />
    <fragment
        android:id="@+id/shopOneFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.ShopOneFragment"
        android:label="fragment_shopone"
        tools:layout="@layout/fragment_shop_one" />
    <fragment
        android:id="@+id/shopTwoFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.ShopTwoFragment"
        android:label="fragment_shoptwo"
        tools:layout="@layout/fragment_shop_two" />
    <fragment
        android:id="@+id/shopThreeFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.ShopThreeFragment"
        android:label="fragment_shopthree"
        tools:layout="@layout/fragment_shop_three" />
    <fragment
        android:id="@+id/shopFourFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.ShopFourFragment"
        android:label="fragment_shopfour"
        tools:layout="@layout/fragment_shop_four" />
    <fragment
        android:id="@+id/shopFiveFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.ShopFiveFragment"
        android:label="fragment_shopfive"
        tools:layout="@layout/fragment_shop_five" />
    <fragment
        android:id="@+id/shopSixFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.ShopSixFragment"
        android:label="fragment_shopsix"
        tools:layout="@layout/fragment_shop_six" />
    <action
    android:id="@+id/action_profileFragment_to_AddressFragment"
    app:destination="@id/addressFragment" />
   <fragment
    android:id="@+id/addressFragment"
    android:name="com.capztone.seafishfy.ui.activities.fragments.AddressFragment"
    android:label="AddressFragment" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_productSearchFragment"
            app:destination="@id/productSearchFragment" />
    </fragment>
    <fragment
        android:id="@+id/freshFishFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.FreshFishFragment"
        android:label="FreshFishFragment" >
    </fragment>

    <fragment
        android:id="@+id/dryFishFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.DryFishFragment"
        android:label="DryFishFragment" >
    </fragment>

    <fragment
        android:id="@+id/picklesFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.PicklesFragment"
        android:label="PicklesFragment" >
    </fragment>

    <fragment
        android:id="@+id/productSearchFragment"
        android:name="com.capztone.seafishfy.ui.activities.fragments.ProductSearchFragment"
        android:label="ProductSearchFragment" >

    </fragment>


    <action
        android:id="@+id/action_homeFragment_to_shoponefragment"
        app:destination="@id/shopOneFragment" />
    <action
        android:id="@+id/action_homeFragment_to_shoptwofragment"
        app:destination="@id/shopTwoFragment" />
    <action
        android:id="@+id/action_homeFragment_to_shopthreefragment"
        app:destination="@id/shopThreeFragment" />
    <action
        android:id="@+id/action_homeFragment_to_shopfourfragment"
        app:destination="@id/shopFourFragment" />
    <action
        android:id="@+id/action_homeFragment_to_shopfivefragment"
